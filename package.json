{
  "name": "@mixmaxhq/verify-build-artifacts",
  "version": "0.1.0",
  "description": "Compare build artifacts to those generated by the pull request base.",
  "bin": "./bin/index.js",
  "main": "./dist/index.js",
  "scripts": {
    "build": "babel -D src -d dist && babel bin/src -d bin",
    "ci": "npm run lint && npm run build && ./build/artifacts.sh",
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mixmaxhq/verify-build-artifacts.git"
  },
  "keywords": [
    "build",
    "diff",
    "CI",
    "continuous",
    "integration",
    "automated",
    "dependabot",
    "diff",
    "difference",
    "build",
    "acceptance"
  ],
  "files": [
    "bin/*.js",
    "dist"
  ],
  "author": "Eli Skeggs <eli@mixmax.com> (https://mixmax.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mixmaxhq/verify-build-artifacts/issues"
  },
  "homepage": "https://github.com/mixmaxhq/verify-build-artifacts#readme",
  "dependencies": {
    "@mixmaxhq/post-github-comment-from-travis": "^0.1.1",
    "@mixmaxhq/promise-pool": "^2.0.0",
    "aws-sdk": "^2.479.0",
    "diff": "^4.0.1",
    "globby": "^9.2.0",
    "joi": "^14.3.1",
    "tar": "^4.4.10",
    "tmp-promise": "^2.0.1",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "eslint": "^5.16.0",
    "eslint-config-mixmax": "^2.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2"
  }
}
